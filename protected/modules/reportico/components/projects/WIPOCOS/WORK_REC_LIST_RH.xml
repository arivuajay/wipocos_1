wk.Work_Id,
Work_Org_Title,
Work_Internal_Code,
CONCAT(SUBSTRING_INDEX(SUBSTRING_INDEX(wk.Work_Duration, ':', 1), ':', -1),&quot;'&quot;,SUBSTRING_INDEX(SUBSTRING_INDEX(wk.Work_Duration, ':', 2), ':', -1),&quot;''&quot;) AS Duration,
wt.Type_Name,
wk.Work_Creation,
wr.Work_Member_GUID,
(SELECT wa.Auth_Sur_Name FROM wipo_author_account AS wa WHERE wr.Work_Member_GUID = wa.Auth_GUID) AS auth_full_name,
(SELECT wp.Pub_Corporate_Name FROM wipo_publisher_account AS wp WHERE wr.Work_Member_GUID = wp.Pub_GUID) AS pub_full_name,
CASE
  WHEN (SELECT wa.Auth_Sur_Name FROM wipo_author_account AS wa WHERE wr.Work_Member_GUID = wa.Auth_GUID) IS NOT NULL THEN (SELECT wa.Auth_Sur_Name FROM wipo_author_account AS wa WHERE wr.Work_Member_GUID = wa.Auth_GUID)
  WHEN (SELECT wp.Pub_Corporate_Name FROM wipo_publisher_account AS wp WHERE wr.Work_Member_GUID = wp.Pub_GUID) IS NOT NULL THEN (SELECT wp.Pub_Corporate_Name FROM wipo_publisher_account AS wp WHERE wr.Work_Member_GUID = wp.Pub_GUID)  
  ELSE '--'
END AS full_name
FROM `wipo_work` AS wk, wipo_master_type AS wt,wipo_work_rightholder AS wr
WHERE 1=1 
AND wk.Work_Type_Id = wt.Master_Type_Id
AND wk.Work_Id = wr.Work_Id
ORDER BY wk.Work_Id
