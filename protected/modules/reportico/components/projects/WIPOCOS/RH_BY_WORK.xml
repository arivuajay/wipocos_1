<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Right Holder List By Work</ReportTitle><ReportDescription></ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT
  wk.Work_Id,
  Work_Org_Title,
  Work_Internal_Code,
  CONCAT(SUBSTRING_INDEX(SUBSTRING_INDEX(wk.Work_Duration, ':', 1), ':', -1),&quot;'&quot;,SUBSTRING_INDEX(SUBSTRING_INDEX(wk.Work_Duration, ':', 2), ':', -1),&quot;''&quot;) AS Duration,
  wt.Type_Name,
  wk.Work_Creation,
CASE
  WHEN wa.Auth_Sur_Name IS NOT NULL THEN CONCAT(wa.Auth_First_Name,' ',wa.Auth_Sur_Name)
  WHEN wp.Pub_Corporate_Name IS NOT NULL THEN wp.Pub_Corporate_Name  
  WHEN pf.Perf_Sur_Name IS NOT NULL THEN CONCAT(pf.Perf_First_Name,' ',pf.Perf_Sur_Name) 
  WHEN pd.Pro_Corporate_Name IS NOT NULL THEN pd.Pro_Corporate_Name     
  ELSE '--'
END AS full_name,
wr.Work_Member_GUID
FROM `wipo_work` AS wk
LEFT JOIN wipo_master_type AS wt ON wt.Master_Type_Id = wk.Work_Type_Id
LEFT JOIN wipo_work_rightholder AS wr ON wr.Work_Id = wk.Work_Id
LEFT OUTER JOIN wipo_author_account AS wa ON wr.Work_Member_GUID = wa.Auth_GUID
LEFT OUTER JOIN wipo_publisher_account AS wp ON wr.Work_Member_GUID = wp.Pub_GUID 
LEFT OUTER JOIN wipo_performer_account AS pf ON wr.Work_Member_GUID = pf.Perf_GUID
LEFT OUTER JOIN wipo_producer_account AS pd ON wr.Work_Member_GUID = pd.Pro_GUID 
WHERE 1 = 1
    AND wk.Work_Type_Id = wt.Master_Type_Id
    AND wk.Work_Id = wr.Work_Id
ORDER BY wk.Work_Id</SQLRaw></SQL><QueryColumns><QueryColumn><Name>Work_Org_Title</Name><TableName></TableName><ColumnName>Work_Org_Title</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Original Title</column_title><tooltip></tooltip><group_header_label>Work</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>Work_Internal_Code</Name><TableName></TableName><ColumnName>Work_Internal_Code</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Internal Code</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>Type_Name</Name><TableName>wt</TableName><ColumnName>Type_Name</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Type</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>Duration</Name><TableName></Table